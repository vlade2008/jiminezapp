{
  "name": "jimenezapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env nodemon DEBUG=jimenezapp node ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "build": "cd frontend && yarn install && yarn run build"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^3.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.2",
    "lodash": "^4.17.11",
    "morgan": "^1.7.0",
    "mysql": "^2.16.0",
    "pg": "^6.1.0",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.0.8"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.18.4",
    "supertest": "^2.0.0"
  }
}
